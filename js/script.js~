$(document).ready(function() {
$('#cadastro').keydown(function() {
var nome = $("#nome").val();
var empresa = $("#empresa").val();
var rg = $("#rg").val();
var cartao = $("#cartao").val();
var key = e.which;
if (key == 13) {
if (nome == "") {
alert("Necessário preencher com nome");
} else if (empresa == "") {
alert("Selecione a empresa");
} else if (rg == "") {
alert("Documento não pode estar vazio");
} else {
$('#cadastro').submit();
}
return false;
}
});
});